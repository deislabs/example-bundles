{
    "name": "hellohelm",
    "version": "0.1.0",
    "invocationImage": {
        "imageType": "docker",
        "image": "technosophos/demo2:0.1.0"
    },
    "images": [
        {
            "name": "alpine",
            "uri": "technosophos/demo2alpine:0.1.0",
            "refs": [
                {
                    "path": "cnab/app/charts/alpine/values.yaml",
                    "field": "image.repository"
                }
            ]
        }
    ],
    "parameters": {
        "port": {
            "defaultValue": 8080,
            "type": "int"
        }
    },
    "credentials": {
        "kubeconfig": {
            "path": "/root/.kube/config"
        }
    }
}